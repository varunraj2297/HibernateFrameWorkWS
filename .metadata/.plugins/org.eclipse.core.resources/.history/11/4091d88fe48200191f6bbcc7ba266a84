package com.nt.test;

import java.util.List;

import javax.persistence.ParameterMode;

import org.hibernate.Query;
import org.hibernate.Session;
import org.hibernate.procedure.ProcedureCall;

import com.nt.entity.Employee;
import com.nt.utility.HibernateUtil;

public class PL_SQLProcedureCallTest {
	public static void main(String[] args) {
		Session ses = null;
		ProcedureCall call=null;
		List<Object[]> list = null;

		ses = HibernateUtil.getSession();
		
		call=ses.createStoredProcedureCall("call  p_get_emps_by_sal_range(?,?,?)");
	
	    call.registerParameter(1,Class.class,ParameterMode.OUT);
	    call.registerParameter(2,Integer.class,ParameterMode.IN).bindValue(4000f);
	    call.registerParameter(3,Integer.class,ParameterMode.IN).bindValue(6000f);
	}

}
